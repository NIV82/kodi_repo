<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<settings>

  <category label="ANIDUB">
    <setting label="[B]AniDub[/B]" id="use_anidub" type="bool" default="true"/>
    <setting type="sep"/>
    <setting id="anidub_auth_mode" label="Авторизация:" type="enum" default="0" values="Отключено|Включено" enable="eq(-2,true)"/>
    <setting id="anidub_username" label="Логин:" type="text" default="" subsetting="true" visible="eq(-1,1)"/>
    <setting id="anidub_password" label="Пароль:" type="text" default="" option="hidden" subsetting="true" visible="eq(-2,1)"/>
    <setting type="sep"/>
    <setting id="anidub_covers" type="enum" label="Обложки:" default="0" values="Не Сохранять|Сохранять" enable="eq(-6,true)" />
    <setting id="anidub_titles" type="enum" label="Формат названий:" default="0" values="Ru|En|Ru-En" enable="eq(-7,true)" />
    <setting type="sep"/>
    <setting id="anidub_mirror_mode" label="Зеркало:" type="enum" default="0" values="Базовый сайт|Зеркало-1|Зеркало-2" enable="eq(-9,true)"/>
    <setting id="anidub_mirror_0" label="Адрес:" type="text" default="https://tr.anidub.com/" subsetting="true" visible="eq(-1,0)" enable="eq(-1,3)"/>
    <setting id="anidub_mirror_1" label="Адрес:" type="text" default="Пока не существует" subsetting="true" visible="eq(-2,1)"/>
    <setting id="anidub_mirror_2" label="Адрес:" type="text" default="Пока не существует" subsetting="true" visible="eq(-3,2)"/>
    <setting type="sep"/>
    <setting id="anidub_unblock" type="enum" label="Разблокировка:" default="0" values="Отключено|Antizapret - Proxy" enable="eq(-14,true)"/>
    <setting type="sep"/>
    <setting id="anidub_engine" type="enum" label="Торрент плеер:" default="2" values="TAM|Elementum|T2Http" enable="eq(-16,true)"/>
    <setting id="anidub_tam" type="enum" label="TAM-плеер:"  visible="eq(-1,0)" default="2" subsetting="true" values="default|ACE engine|T2Http|YATP|Torrenter|Elementum|XBMCtorrent|ACE proxy|Quasar|TorrServer" enable="eq(-17,true)"/>
    <setting id="anidub_session" label="session" type="text" default="" visible="false"/>
    <setting id="anidub_search" label="status" type="text" default="" visible="false"/>
    <setting id="anidub_auth" label="auth" type="bool" default="false" visible="false"/>
  </category>

